<Application
    x:Class="Swoa.UI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Swoa.UI"
    ShutdownMode="OnLastWindowClose"
    Startup="Application_Startup">
    <Application.Resources>
        <local:DivideValueConverter x:Key="divideValueConverter" />
        <local:MultipleValueConverter x:Key="multipleValueConverter" />
        <local:SubstractValueConverter x:Key="substractValueConverter" />
    </Application.Resources>
</Application>
